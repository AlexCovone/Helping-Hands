<% if (typeof user !== 'undefined') { %>
  <%- include('partials/header') -%>
<% } else { %>
  <%- include('partials/indexHeader') -%>
<% } %>

<% if (typeof user !== 'undefined') { %>
<main class="grid place-items-center py-24 px-6 sm:py-32 lg:px-8 -mt-40" style="height: 100vh">
<% } else { %>
<main class="grid place-items-center py-24 px-6 sm:py-32 lg:px-8" style="height: 100vh">
<% } %>
    <div class="text-center">
      <p class="text-3xl font-semibold text-primary">404</p>
      <h1 class="mt-4 text-3xl font-bold tracking-tight text-primary sm:text-5xl">Page not found</h1>
      <p class="mt-6 text-base leading-7 text-gray-400">Sorry, we couldn’t find the page you’re looking for.</p>
      <div class="mt-10 flex items-center justify-center gap-x-6">
        <% if (typeof user !== 'undefined') { %>
          <a href="/events" class="rounded-md btn btn-primary tracking-wide px-3.5 py-2.5 text-sm font-semibold shadow-sm focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2">Go back home</a>
        <% } else { %>
          <a href="/" class="rounded-md btn btn-primary tracking-wide px-3.5 py-2.5 text-sm font-semibold shadow-sm focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2">Go back home</a>
        <% } %>
      </div>
    </div>
</main>
  
<%- include('partials/footer') -%>